     "output_type": "stream",
     "text": [
      "enter the number :5\n",
      "615\n"
     ]
    }
   ],
   "source": [
    "a = int(input(\"enter the number :\"))\n",
    "n = int(\"%s\" % a)\n",
    "nn = int(\"%s%s\" % (a,a))\n",
    "nnn = int(\"%s%s%s\" % (a,a,a))\n",
    "print(n+nn+nnn)"
   ]
